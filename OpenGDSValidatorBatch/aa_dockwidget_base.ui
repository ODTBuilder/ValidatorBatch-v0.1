}











/** group boxes */



QGroupBox {
    background-color: #ffffff;
    border: 1px solid #d8d8d8;
    border-top-style: solid;
    border-right-style: solid;
    border-bottom-style: solid;
    border-left-style: solid;
    border-radius: 0;
    margin-top: 3ex;  
}

QGroupBox::title {
     subcontrol-origin: margin;
    subcontrol-position: top left;  
    padding: 0 6px;
	color: #335095;
    background-color:#ffffff;
}





/** PushButtons **/

QPushButton {
    background-color: ;
	border:1px solid #999;
    border-width: 1px;
    border-radius: 2px;
       min-width: 2em;
    padding: 6px;
}

QPushButton::hover {
    background-color:#dde3f4;
    border-color: #91aee0;
}

QPushButton::disabled {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #ffffff22, stop: 1.0 #9b9bff22);
    border-style: dotted;
    border-width: 1px;
    border-radius: 5px;
    border-color: #99999933;
    min-width: 2em;
    padding: 6px;
    color:#33333;
}




QLineEdit {
    color: #333;
	padding:2px 5px;
    background-color: #fff;
    selection-color: #335095;
	selection-border: 1px solid #90a5dc;
    selection-background-color: #e9eefb;
    border: 1px solid #c5c5c5; /* border top color defined after QAbstractSpinBox, QLineEdit and QComboBox */
    border-radius: 2px;
}
 
QLineEdit:read-only {
       background-color: #ccc;
}

QTextEdit, QListView {
    background-color: white;
    border: 1px solid gray;
    background-attachment: scroll;
}







/* ==================================================================================== */
/* SCROLL BAR                                                                           */
/* ==================================================================================== */

QScrollBar:horizontal {
    background-color: #333;
    height: 8px;
    margin: 0px;
    padding: 0px;
}

QScrollBar::handle:horizontal {
    border: 1px solid #111;
    background: #535353;
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal,
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    width: 0px;
    background: transparent;
}

QScrollBar:vertical {
    background-color: #c5c5c5;
    width: 8px;
    margin: 0;
}

QScrollBar::handle:vertical {
    border: 1px solid #adadad;
    background: #adadad;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical,
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    height: 0px;
    background: transparent;
}








/** Dock Widgets e.g. Time Manager */

QDockWidget
{	padding:10px 0 0 0;
  	color: #000;
  
}



QDockWidget::title {
    text-align: center;
    padding: 4px;
	border-bottom:1px solid #c5c5c5;
	border-right:1px solid #c5c5c5;

}

QDockWidget::close-button,
QDockWidget::float-button {
    border: 1px transparent #e6e6e6;
    border-radius: 2px;
    background: transparent;
    subcontrol-origin: padding;
    subcontrol-position: right center;
}

QDockWidget::close-button {
    right: 4px;
}
    
QDockWidget::float-button {
    right: 22px;
}

QDockWidget::close-button:hover,
QDockWidget::float-button:hover {
    background: #f5f5f5;
}

QDockWidget::close-button:pressed,
QDockWidget::float-button:pressed {
    /*padding: 1px -1px -1px 1px;*/
    background-color: #e0e0e0;
}











/** handle */

/* Similar to the splitter is the following window separator: */

QMainWindow::separator
{    background-position: center center;
    background-repeat: none;
    background-color: #d8def1;
    color: white;
	border-left:1px solid #c6cde3;
    padding-left: 4px;   
    spacing: 3px; /* spacing between items in the tool bar */
}

QMainWindow::separator:hover
{

    background-color: #d8def1;
    color: white;
    padding-left: 4px;

    border-right: 1px solid #284898;
    spacing: 3px; /* spacing between items in the tool bar */
}

